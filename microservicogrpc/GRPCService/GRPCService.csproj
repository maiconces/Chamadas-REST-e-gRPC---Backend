    <PackageReference Include="App.Metrics.AspNetCore.Endpoints" Version="4.2.0-preview.0.10" />
    <PackageReference Include="App.Metrics.Formatters.Prometheus" Version="4.1.0" />

    <PackageReference Include="App.Metrics.GrafanaCloudHostedMetrics" Version="4.1.0" />
    <PackageReference Include="App.Metrics.InfluxDB" Version="4.1.0" />
    <PackageReference Include="App.Metrics.Prometheus" Version="4.1.0" />

    <PackageReference Include="App.Metrics.Reporting.Socket" Version="4.1.0" />
    
    <PackageReference Include="Google.Protobuf" Version="3.13.0" />
    <PackageReference Include="Grpc.AspNetCore" Version="2.31.0" />
    <PackageReference Include="Grpc.AspNetCore.Web" Version="2.31.0" />
    <PackageReference Include="Grpc.Net.Client" Version="2.31.0" />
    <PackageReference Include="Grpc.Tools" Version="2.32.0">
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
      <PrivateAssets>all</PrivateAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.ApplicationInsights.AspNetCore" Version="2.15.0" />
    <PackageReference Include="Microsoft.ApplicationInsights.PerfCounterCollector" Version="2.15.0" />
